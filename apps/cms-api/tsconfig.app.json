{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "strict": true,
    "strictPropertyInitialization": false,
    "sourceMap": true
  },
  "files": [
    "package.json"
  ],
  "include": [
    "types",
    "src",
    "prisma"
  ],
  "exclude": [
    "**/*.spec.ts"
  ]
}

    "@swc/core": "^1.7.22",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.7.14",
    "cross-env": "^7.0.3",
    "sqlite3": "^5.1.6",
    "supertest": "^7.0.0",
    "unplugin-swc": "^1.5.1"
  }
